<%- include layouts/header.ejs %>
<h1 class="title" id="page-title">Programs</h1>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Script</th>
        <th>Comment</th>
    </tr>

    <% if(data.length){for(var i = 0;i < data.length;i++) { %>

    <tr>
        <td><%- data[i].Id%></td>
        <td><%- data[i].Name%></td>
        <td><%- data[i].Script%></td>
        <td><%- data[i].Comment%></td>
    </tr>
    
    <% }}else{ %>
    
    <tr>
        <td colspan="3">No programs</td>
    </tr>
    
    <% } %>
</table>

<%- include layouts/footer.ejs %>
